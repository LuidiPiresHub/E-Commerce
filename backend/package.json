{
  "name": "backend",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "mocha --require ts-node/register __tests__/**/*.{test,spec}.ts",
    "test:unit": "mocha --require ts-node/register __tests__/unit/*.{test,spec}.ts",
    "test:integration": "mocha --require ts-node/register __tests__/integration/*.{test,spec}.ts",
    "coverage": "nyc npm run test",
    "lint": "npx eslint src/**/*.ts --fix",
    "compose:up": "docker-compose up -d",
    "compile": "npx tsc",
    "start": "node dist/src/server.js",
    "prestart": "npm run compile && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate",
    "dev": "nodemon --exec ts-node src/server.ts",
    "restart": "npm run compile && npx sequelize db:drop && npx sequelize db:create && npx sequelize db:migrate && npx sequelize db:seed:all"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.3",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mocha": "^10.0.1",
    "@types/sinon-chai": "^3.2.9",
    "chai": "^4.3.7",
    "chai-http": "^4.4.0",
    "eslint": "^8.46.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-n": "^16.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "nyc": "^15.1.0",
    "sequelize-cli": "^6.6.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "jsonwebtoken": "^9.0.1",
    "mysql2": "^3.5.2",
    "sequelize": "^6.32.1"
  }
}
